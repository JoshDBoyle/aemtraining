<?xml version="1.0" encoding="UTF-8"?>
<jcr:root 
	xmlns:oak="http://jackrabbit.apache.org/oak/ns/1.0" 
	xmlns:slingevent="http://sling.apache.org/jcr/event/1.0" 
	xmlns:sling="http://sling.apache.org/jcr/sling/1.0" 
	xmlns:granite="http://www.adobe.com/jcr/granite/1.0" 
	xmlns:dam="http://www.day.com/dam/1.0" 
	xmlns:cq="http://www.day.com/jcr/cq/1.0" 
	xmlns:jcr="http://www.jcp.org/jcr/1.0" 
	xmlns:mix="http://www.jcp.org/jcr/mix/1.0" 
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0" 
	xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="nt:unstructured">
	<cpcReportMetadata
        jcr:primaryType="oak:QueryIndexDefinition"
        async="async"
        compatVersion="{Long}2"
        evaluatePathRestrictions="{Boolean}true"
        excludedPaths="[/jcr:system,/libs,/apps,/etc,/content/projects,/content/usergenerated,/content/campaigns,/content/launches,/content/publications,/content/forms,/content/screens,/content/communities,/content/sites,/content/catalogs,/var]"
        reindex="{Boolean}false"
        reindexCount="{Long}1"
        type="lucene">
        <indexRules jcr:primaryType="nt:unstructured">
            <nt:unstructured jcr:primaryType="nt:unstructured">
                <properties jcr:primaryType="nt:unstructured">
                    <jcrLockOwner
                        jcr:primaryType="nt:unstructured"
                        analyzed="{Boolean}true"
                        name="cq:lastModified"
                        nodeScopeIndex="{Boolean}true"/>
                    <cqLastReplicated
                        jcr:primaryType="nt:unstructured"
                        analyzed="{Boolean}true"
                        name="cq:lastModified"
                        nodeScopeIndex="{Boolean}true"/>
                    <cqLastReplicationAction
                        jcr:primaryType="nt:unstructured"
                        analyzed="{Boolean}true"
                        name="cq:lastModified"
                        nodeScopeIndex="{Boolean}true"/>
                    <cqLastModified
                        jcr:primaryType="nt:unstructured"
                        analyzed="{Boolean}true"
                        name="cq:lastModified"
                        nodeScopeIndex="{Boolean}true"/>
                </properties>
            </nt:unstructured>
        </indexRules>
    </cpcReportMetadata>
	<!--cqLastModified
	        jcr:primaryType="oak:QueryIndexDefinition"
	        declaringNodeTypes="{Name}[cq:PageContent,nt:unstructured]"
	        propertyNames="{Name}[cq:lastModified]"
	        reindex="{Boolean}false"
	        reindexCount="{Long}1"
	        type="property"/>
	<cqLastReplicated
	        jcr:primaryType="oak:QueryIndexDefinition"
	        declaringNodeTypes="{Name}[cq:PageContent,nt:unstructured]"
	        propertyNames="{Name}[cq:lastReplicated]"
	        reindex="{Boolean}false"
	        reindexCount="{Long}1"
	        type="property"/>
	<cqLastReplicatedBy
	        jcr:primaryType="oak:QueryIndexDefinition"
	        declaringNodeTypes="{Name}[cq:PageContent,nt:unstructured]"
	        propertyNames="{Name}[cq:lastReplicatedBy]"
	        reindex="{Boolean}false"
	        reindexCount="{Long}1"
	        type="property"/-->
</jcr:root>